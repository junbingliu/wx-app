.home-dynamic .dynamic-list .text .txt{
  padding-top: 17.5rpx;
  display: block;
  font-size: 22rpx;
  width: 351rpx;
  overflow: hidden;
  color: #333;
  line-height: 33rpx;
  word-break:break-all;
  display:-webkit-box;
  -webkit-line-clamp:3;
  -webkit-box-orient:vertical;
  height: 95rpx;
}
.home-dynamic .dynamic-list .dynamic-ico{
  position: absolute;
  right: 23.4rpx;
  bottom: 23.4rpx;
  width: 37.44rpx;
  height: 37.44rpx;
}
/* 底部菜单栏 */
.home-footer {
    width: 100%;
    margin: auto;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 99;
}
.foot-nav {
    background: #fefefe;
    border-top: #dfdfdf 1px solid;
    height: 117rpx;
    position: relative;
  background: linear-gradient(to bottom, #54a0f6 , #0058bc);
      display: -webkit-box;
}
.home-footer .foot-nav .f-nav{
    -webkit-box-flex: 1;
    width: 100%;
}

.home-footer .foot-nav .f-nav .nav{
    width:105.3rpx;
    margin: 0 auto;
    padding:18.7rpx  0 0 0;
    text-align: center;
    font-size: 23.4rpx;
    color: #333;
    position: relative;
}
.home-footer .foot-nav .f-nav .ico{
   width: 47rpx;
   height: 47rpx;
   display: inline-block;
}
.home-footer .foot-nav .f-nav .txt {
    display: block;
    color: #4e9cf3;
}
.home-footer .foot-nav .f-nav.active .txt{
  color: #fff;
}
.home-footer .foot-nav .f-nav.home-logo .ico{
  width: 142rpx;
  height: 142rpx;
  margin-top: -60rpx;
}
/* end 底部菜单栏 */
